{
  "name": "express_simple",
  "description": "Simple Example Showcasing How To Scale Express App With Dynoscale",
  "version": "0.0.1",
  "engines": {
    "node": "18.15.0"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "relative-deps",
    "prestart": "relative-deps",
    "prebuild": "relative-deps",
    "pretest": "relative-deps"
  },
  "keywords": [],
  "author": "Ondrej Dolejsi",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "@dynoscale/dynoscale": "^0.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "relative-deps": "^1.0.7"
  },
  "relativeDependencies": {
    "dynoscale": "../../dynoscale/"
  },
  "nodemonConfig": {
    "verbose": true,
    "delay": 50,
    "ext": "js,ts,jsx,tsx,mjs,cjs,mjsx,cjsx,json",
    "watch": [
      ".",
      "../../dynoscale"
    ]
  }
}